<h1>Hi</h1>
<section>
  <.greet message={@message} />
</section>

<table>
  <tr>
    <th>Number</th>
    <th>Power</th>
  </tr>
  <%= for number <- 1..10 do %>
    <tr>
      <td>{number}</td>
      <td>{number * number}</td>
    </tr>
  <% end %>
</table>

<.modal id="foo" show={true}>
  <.greet message={@message} />
</.modal>
